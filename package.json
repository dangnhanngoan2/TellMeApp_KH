{
  "name": "TellMe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clear-simulator-data": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "change_port": "react-native start --port 9988",
    "startrs": "npm start -- --reset-cache",
    "checkdeps": "ncu --packageFile package.json",
    "i5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "i11": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "i6": "react-native run-ios --simulator=\"iPhone 6\"",
    "i6s": "react-native run-ios --simulator=\"iPhone 6s\"",
    "xsm": "react-native run-ios --simulator=\"iPhone XS Max\"",
    "mi_phone": "react-native run-ios --simulator=\"Mi's iPhone\"",
    "i6p": "react-native run-ios --simulator=\"iPhone 6 Plus\"",
    "x": "react-native run-ios --simulator=\"iPhone X\"",
    "ls-ios": "instruments -s devices",
    "a": "react-native run-android",
    "ar-dev": "react-native run-android --variant=\"release\"",
    "ar": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "shake": "adb shell am broadcast -a react.native.RELOAD",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "appcenter-ios": "cd ios && fastlane appcenter && cd ..",
    "appcenter-a": "cd android && fastlane appcenter && cd ..",
    "release": "cd ios && fastlane release && cd ..",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run git-hook && npm run fixcode",
    "fixcode": "standard --fix",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "lint": "standard --verbose | snazzy",
    "git-hook": "npm run lint -s"
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "^4.13.0",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "formik": "^1.5.8",
    "geolib": "^3.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "querystringify": "^2.1.1",
    "react": "16.8.6",
    "react-native": "0.61.2",
    "react-native-animatable": "^1.3.3",
    "react-native-background-timer": "^2.1.1",
    "react-native-camera-kit": "^8.0.4",
    "react-native-chonse-select": "^1.0.5",
    "react-native-code-push": "^6.3.0",
    "react-native-date-picker": "^2.6.1",
    "react-native-dialog-component": "^0.2.17",
    "react-native-elements": "^1.1.0",
    "react-native-fast-image": "^8.3.2",
    "react-native-geocoder": "^0.5.0",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-gifted-chat": "^0.12.0-beta-0",
    "react-native-global-props": "^1.1.5",
    "react-native-i18n": "git+https://github.com/lemankk/react-native-i18n.git#e870017bd93c9b2e3762ae83a2db3de8c4257146",
    "react-native-image-picker": "^1.0.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-location": "^2.5.0",
    "react-native-maps": "0.25.0",
    "react-native-onesignal": "3.9.3",
    "react-native-permissions": "^2.0.8",
    "react-native-progress": "^4.1.2",
    "react-native-ratings": "^7.3.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-screens": "^2.16.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-switch-selector": "^2.0.6",
    "react-native-vector-icons": "^7.1.0",
    "react-navigation": "^3.11.0",
    "react-navigation-stack": "^2.10.2",
    "react-navigation-tabs": "^2.10.1",
    "react-redux": "^7.1.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.18",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.3",
    "sockjs-client": "^1.5.0",
    "stompjs": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jest": "^24.1.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.6"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
